<!DOCTYPE html>
<html>
  <head>
    <title>FlapperNews</title>
    <link href="//maxcdn.bootstrapcdn.com/bootstrap/3.2.0/css/bootstrap.min.css" rel="stylesheet">
    <%= stylesheet_link_tag    'application', media: 'all' %>
    <%= javascript_include_tag 'application' %>
    <%= csrf_meta_tags %>
    <script src='https://www.google.com/recaptcha/api.js?hl=en'></script>
  </head>
  <body>
    <% shield_square_user_name = "Saravanaselvan SP" %>
    <% shieldsquare_pid = "" %>
    <% @shieldsquare_response = Ss2.shieldsquare_ValidateRequest(shield_square_user_name, 2, shieldsquare_pid, request, cookies) %>
    <div class="row">
      <div class="col-md-6 col-md-offset-3">
        <table class="table table-striped table-bordered">
          <tbody>
            <tr>
              <th>PID</th>
              <td><%= @shieldsquare_response["pid"] %></td>
            </tr>
            <tr>
              <th>Response Code</th>
              <td><%= @shieldsquare_response["responsecode"] %></td>
            </tr>
            <tr>
              <th>URL</th>
              <td><%= @shieldsquare_response["url"] %></td>
            </tr>
            <tr>
              <th>Reason</th>
              <td><%= @shieldsquare_response["reason"] %></td>
            </tr>
            <tr>
              <th>dynamic_JS</th>
              <td><%= @shieldsquare_response["dynamic_JS"] %></td>
            </tr>
          </tbody>
        </table>
        <% unless @captcha_response.blank? %>
          <%= @captcha_response %>
        <% end %>        
        <form action="/products" style="float:left;" method="post">
          <div class="g-recaptcha" data-sitekey="6Ld23BYTAAAAADuWnWn5sPKW69RS_GigjPSSAsTP" data-type="image"></div>
          <input type="submit" value="Submit" class="btn btn-primary">
        </form>        
      </div>
    </div>
    <script type="text/javascript">
      var __uzdbm_a = '<%= @shieldsquare_response["pid"] %>';
    </script>
    <div id="ss_098786_234239_238479_190541"></div>  
    <script type="text/javascript" src ="https://cdn.perfdrive.com/static/jscall_min.js" async="true">
    </script>     
  </body>
</html>